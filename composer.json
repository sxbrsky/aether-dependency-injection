{
    "name": "sxbrsky/dependency-injection",
    "description": "A lightweight PSR-11 dependency injection container.",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "di",
        "ioc",
        "psr-11",
        "container",
        "dependency-injection"
    ],
    "authors": [
        {
            "name": "Dominik Szamburski",
            "email": "dominikszamburski99@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/sxbrsky/php-dependency-injection/issues",
        "source": "https://github.com/sxbrsky/php-dependency-injection"
    },
    "require": {
        "php": ">=8.2",
        "psr/container": "^2.0.x-dev"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "dev-master",
        "phpbench/phpbench": "^1.3",
        "phpstan/phpstan": "2.0.x-dev",
        "phpunit/phpunit": "^11.4.x-dev"
    },
    "provide": {
        "psr/container-implementation": "2.0.2"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Sxbrsky\\DependencyInjection\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sxbrsky\\DependencyInjection\\Tests\\Benchmark\\": "tests/benchmark/",
            "Sxbrsky\\DependencyInjection\\Tests\\Unit\\": "tests/unit/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
